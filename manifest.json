{
  "name": "Chess.com Next Move",
  "version": "0.0.1",
  "description": "Allows you to consider next best move with a given board state",
  "permissions": ["storage", "declarativeContent", "activeTab", "https://www.chess.com/"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/knight16.png",
      "32": "images/knight32.png",
      "48": "images/knight48.png",
      "128": "images/knight128.png"
    }
  },
  "icons": {
    "16": "images/knight16.png",
    "32": "images/knight32.png",
    "48": "images/knight48.png",
    "128": "images/knight128.png"
  },
  "manifest_version": 2
}